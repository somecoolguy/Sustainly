.st {
  position: fixed;
  bottom: 0%;
  width: 100%;
}

.st-content {
  margin: 10px; /*Margins for inner DIV inside each column (to provide padding)*/
  margin-top: 0;
}

.st #st-header {
  color:#D2D6D9;
  font-weight: bold;
  text-align: center;
  border-radius:10px 10px 0 0; 
  /*box-shadow:0 1px 5px 0 rgba(0,0,0,0.4);*/
  background:#323531;
  height:33px;
  width:500px;
  margin:0 30%;
  -moz-box-shadow: 0 0 5px rgba(0,0,0,0.5);
  -webkit-box-shadow: 0 0 5px rgba(0,0,0,0.5);
  box-shadow: 0 0 5px rgba(0,0,0,0.5);
}

.st #st-body {
  color:#D2D6D9;
  border-radius:10px 10px 0 0; 
  background: #3D4242;
  height:250px;
  resize:vertical;
  max-height:400px;
  margin:0 2.5%;
  -moz-box-shadow: 0 0 5px rgba(0,0,0,0.5);
  -webkit-box-shadow: 0 0 5px rgba(0,0,0,0.5);
  box-shadow: 0 0 5px rgba(0,0,0,0.5);
  overflow: scroll;
}

.st #st-header-left {
  float: left;
  width: 32%; /*Width of left column in percentage*/
  height: 100%;
  margin-left: 2%;
  background: #3D4242;
}

.st #result-header-left {
  float:left;
  width:48px;
  height:100%;
/*  background: #3D4242; */
/*  border-left: 1px solid #FFFFFF; */
  margin-left:2px;
}

.st #result-header-center {
  float:left;
  height:100%;
/*  margin: 0 48px 0 48px; *//*Margins for content column. Should be "0 RightColumnWidth 0 LeftColumnWidth*/
  width: 398px;
  background: #3D4242;
  border-left: 1px solid #000000;
}

.st #result-header-right {
  float:left;
  width:48px;
  height:100%;
/*  background: #3D4242; */
  border-left: 1px solid #000000;
  margin-right:2px;
}

.st #product-header-left {
  float:left;
  width:48px;
  height:100%;
  margin-left:2px;
}

.st #product-header-center {
  float:left;
  height:100%;
/*  margin: 0 48px 0 48px; *//*Margins for content column. Should be "0 RightColumnWidth 0 LeftColumnWidth*/
  width:198px;
  background: #3D4242;
  border-left: 1px solid #000000;
}

.st #product-header-right {
  float:left;
  width:198px;
  height:100%;
  background: #3D4242;
  border-left: 1px solid #000000;
}

.st #product-header-far-right {
  float:left;
  width:48px;
  height:100%;
  border-left: 1px solid #000000;
  margin-right:2px;
}

.st #result-header-center:hover {
  background:rgba(0, 0, 0, 0.1);
  cursor:pointer;
}

.st #product-header-center:hover {
  background:rgba(0, 0, 0, 0.1);
  cursor:pointer;
}

.st #product-header-right:hover {
  background:rgba(0, 0, 0, 0.1);
  cursor:pointer;
}

.st #st-header-left:hover {
  background:rgba(0, 0, 0, 0.1);
  cursor:pointer;
}

.st #st-header-center {
  height: 100%;
  width: 32%;
  margin: 0 32% 0 32%; /*Margins for content column. Should be "0 RightColumnWidth 0 LeftColumnWidth*/
  background: #3D4242;
  border-left: 1px solid #FFFFFF;
  padding:7px;
}

.st #st-header-center:hover {
  background:rgba(0, 0, 0, 0.1);
  cursor:pointer;
}

.st #st-header-right {
  float: right;
  width: 32%; /*Width of right column in pixels*/
  height: 100%;
  margin: 0 2% 0 0%;
  background: #3D4242;
}

.st #st-header-right:hover {
  background:rgba(0, 0, 0, 0.1);
  cursor:pointer;
}

.st #st-body-left {
  float: left;
  width: 20%; /*Width of left column in percentage*/
  height: 100%;
  margin-left: 2%;
  background: #3D4242;
}

.st #st-body-center {
  height: 100%;
  margin: 0 22% 0 22%; /*Margins for content column. Should be "0 RightColumnWidth 0 LeftColumnWidth*/
  background: #4F5559;
}

.st #st-body-right {
  float: right;
  width: 20%; /*Width of right column in pixels*/
  height: 100%;
  margin-right: 2%; /*Set margin to that of -(RightColumnWidth)*/
  background: #3D4242;
}

.st-contenth {
  margin: 10px; /*Margins for inner DIV inside each column (to provide padding)*/
  margin-top: 0;
}

.st-contenth:hover {
  cursor: pointer;
}

form fieldset {
  margin-bottom: 10px;
}
form legend {
  padding: 0 2px; 
  font-weight: bold;
}
form label {
  display: inline-block;
  line-height: 1.8;
  vertical-align: top;
}
form fieldset ol {
  margin: 0;
  padding: 0;
}
form fieldset li {
  list-style: none;
  margin: 0;
}
form fieldset fieldset {
  border: none;
  margin: 3px 0 0;
}
form fieldset fieldset legend {
  padding: 0 0 5px;
  font-weight: normal;
}
form fieldset fieldset label {
  display: block;
  width: auto;
}
form em {
  font-weight: bold;
  font-style: normal;
  color: #f00;
}
form label {
  width: 40%; /* Width of labels */
}
form fieldset fieldset label {
  margin-left: 42%; /* Width plus 3 (html space) */
}

#graph {
	min-width: 530px; 
	height: 400px; 
	margin: 0 auto;
}

#graphButton1 {
	width: 212px;
	height: 40px;
	color: white;
	white-space: nowrap;
	line-height:1em;
	position:relative;
	outline: none;
	cursor: pointer;
	border: rgba(0, 0, 0, .2) 1px solid;/* Saf4+, Chrome, FF3.6 */
	border-bottom:rgba(0, 0, 0, .4) 1px solid;
	-webkit-box-shadow: 0 1px 2px rgba(0,0,0,.2);
	-moz-box-shadow: 0 1px 2px rgba(0,0,0,.2);
	box-shadow: 0 1px 2px rgba(0,0,0,.2);
	background: -moz-linear-gradient(
		center top,
		rgba(255, 255, 255, .1) 0%,
		rgba(0, 0, 0, .1) 100%
		);/* FF3.6 */
		background: -webkit-gradient(
			linear,
			center bottom,
			center top,
			from(rgba(0, 0, 0, .1)),
			to(rgba(255, 255, 255, .1))
			);/* Saf4+, Chrome */
			background-color: #91bd09;
			font-family:Arial,Helvetica,sans-serif;
			font-size:14px;
}

#graphButton1:hover {
	background: -webkit-gradient(
		linear,
		center bottom,
		center top,
		from(rgba(255, 255, 255, .1)),
		to(rgba(100, 100, 100, .2))
	);/* Saf4+, Chrome */
	background-color: #91bd09;
	color:white;

}

